{
  "version": 2,
  "build": {

      "src": "app.js",
      "use": "@now/node"
  },
  "routes": [
    { "src": "/api/auth(?:/.*)?", "dest": "/api/auth.js" },
    { "src": "/api/trades(?:/.*)?", "dest": "/api/trades.js" },
    { "src": "/api/users(?:/.*)?", "dest": "/api/users.js" },
    { "src": "/api/settings/target(?:/.*)?", "dest": "/api/target.js" },
    { "src": "/api/health(?:/.*)?", "dest": "/api/health.js" }
  ]
}
